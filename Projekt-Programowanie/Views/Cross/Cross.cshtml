@using Projekt_Programowanie.Models.MODELS
@model IEnumerable<Krzyzowka>
@{
    ViewData["Title"] = "Dodaj pytanie lub odpowiedź";
}

<div class="row">
    <div class="col-md-8 offset-2">
        <p>
            <h4>Generator Krzyżówki</h4>
        </p>
        <div class="form-group">
            @foreach(var item in Model)
            {
                <div class="text-muted">@item.ID_Krzyzowki</div>
            }
        </div>
</div>
</div>
